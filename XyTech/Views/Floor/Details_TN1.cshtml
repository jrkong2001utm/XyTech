@model XyTech.Models.tb_floor

@{
    ViewBag.Title = "Details_TN1";
}

<!DOCTYPE html>
<html>
<body>
    <p class="card-title mb-0" style="font-size: x-large; font-weight:bold">Floor Information</p>
    <br />
    <div>
        <img class="floorlayout" src="~/Content/assets/images/floor_layout_trim/TN1-2.jpeg" alt="DL1" usemap="#DL1map">

        <map name="DL1map">
            <area shape="rect" coords="176,35,262,95" alt="Room1" href="@Url.Action("Details", "Room", new { id = 70 })" id="r_id">
            <area shape="rect" coords="180,99,262,159" alt="Room2" href="@Url.Action("Details", "Room", new { id = 71 })" id="r_id">
            <area shape="rect" coords="177,163,263,223" alt="Room3" href="@Url.Action("Details", "Room", new { id = 73 })" id="r_id">
            <area shape="rect" coords="177,227,262,285" alt="Room4" href="@Url.Action("Details", "Room", new { id = 75 })" id="r_id">
            <area shape="rect" coords="178,294,261,350" alt="Room5" href="@Url.Action("Details", "Room", new { id = 77 })" id="r_id">
            <area shape="poly" coords="204,359,262,358,263,416,178,414,177,384" alt="Room6" href="@Url.Action("Details", "Room", new { id = 78 })" id="r_id">
            <area shape="rect" coords="179,484,261,545" alt="Room7" href="@Url.Action("Details", "Room", new { id = 79 })" id="r_id">
            <area shape="rect" coords="178,549,261,611" alt="Room8" href="@Url.Action("Details", "Room", new { id = 80 })" id="r_id">
            <area shape="rect" coords="100,557,154,641" alt="Room9" href="@Url.Action("Details", "Room", new { id = 81 })" id="r_id">
            <area shape="poly" coords="204,615,261,612,262,672,177,673,178,643" alt="Room10" href="@Url.Action("Details", "Room", new { id = 82 })" id="r_id">
            <area shape="poly" coords="66,491,144,492,146,560,127,572,119,590,64,590" alt="Room11" href="@Url.Action("Details", "Room", new { id = 83 })" id="r_id">
            <area shape="rect" coords="65,427,148,486" alt="Room12" href="@Url.Action("Details", "Room", new { id = 84 })" id="r_id">
            <area shape="rect" coords="66,163,146,247" alt="Room13" href="@Url.Action("Details", "Room", new { id = 85 })" id="r_id">
            <area shape="rect" coords="67,75,146,157" alt="Room13" href="@Url.Action("Details", "Room", new { id = 86 })" id="r_id">
        </map>
    </div>

    <div class="col-7 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <form class="forms-sample">
                    <div class="form-group row">
                        <label for="exampleInputUsername2" class="col-sm-4 col-form-label">Building Name</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.fl_bname)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputEmail2" class="col-sm-4 col-form-label">Floor Level</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.fl_bid)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputMobile" class="col-sm-4 col-form-label">Address</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.fl_address)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputPassword2" class="col-sm-4 col-form-label">Landlord Name</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.tb_landlord.l_name)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">WiFi Password</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.fl_wifipwd)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">Modem IP</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.fl_modemip)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">CCTV QR Code</label>
                        <div class="col-sm-7 col-form-label">
                            <img src="@Url.Content("~/Content/assets/images/cctvqr/" + Model.fl_cctvqr)" alt="Uploaded Image" style="width: 300px; height: auto;" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">Preset Message</label>
                        <div class="col-sm-7 col-form-label">
                            [
                            <br />
                            (attached layout bilik TN1)
                            <br />
                            dalam gambar 👆🏼
                            <br />
                            --> harga sewa bulanan
                            <br />
                            termasuk api dan air
                            <br />
                            --> bilik masih kosong
                            <br />
                            yang tidak bertanda ❌
                            <br />
                            <br />
                            + deposit RM100
                            <br />
                            <br />
                            Tingkat 1
                            <br />
                            <br />
                            gambar bilik:<br />
                            https://drive.google.com/folderview?id=1Myk9ywnSXkebUQzC5AnwawL-l8W2NMDM
                            <br />
                            <br />
                            video bilik:<br />
                            https://youtube.com/playlist?list=PLPU6s1lrzEXK2KtH4ESuuJO3GoeFblVGM
                            <br />
                            <br />
                            lokasi:<br />
                            https://maps.app.goo.gl/chouqqNCKn8mvoqA9
                            <br />
                            atas KK Mart
                            <br />
                            ]
                        </div>
                    </div>
                    <div style="margin-top: 25px;">
                        @Html.ActionLink("Edit", "Edit", new { id = Model.fl_id }, new { @class = "btn btn-primary btn-rounded btn-fw" })
                        @Html.ActionLink("See Rooms", "Index", "Room", new { id = Model.fl_id }, new { @class = "btn btn-dark btn-rounded btn-fw" })
                        @Html.ActionLink("Back", "FloorList", null, new { @class = "btn btn-light btn-rounded btn-fw" })
                    </div>
                </form>
            </div>
        </div>
    </div>


</body>
</html>
