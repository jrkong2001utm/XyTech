

@model IEnumerable<XyTech.Models.tb_floor>

@{
    ViewBag.Title = "Details_TN1";
}

<!DOCTYPE html>
<html>
<body>
    <p class="card-title mb-0" style="font-size: x-large; font-weight:bold">Floor Information</p>
    <br />
    <div>
        <img class="floorlayout" src="~/Content/assets/images/floor_layout_trim/TN1-2.jpeg" alt="DL1" usemap="#DL1map" width="300" height="800">

        <map name="DL1map">
            <area shape="rect" coords="176,35,262,95" alt="Room1" href="~/Views/Room/Details/70" id="r_id">
            <area shape="rect" coords="180,99,262,159" alt="Room2" href="~/Views/Room/Details/71" id="r_id">
            <area shape="rect" coords="177,163,263,223" alt="Room3" href="~/Views/Room/Details/73" id="r_id">
            <area shape="rect" coords="177,227,262,285" alt="Room4" href="~/Views/Room/Details/75" id="r_id">
            <area shape="rect" coords="178,294,261,350" alt="Room5" href="~/Views/Room/Details/77" id="r_id">
            <area shape="rect" coords="176,358,262,415" alt="Room6" href="~/Views/Room/Details/78" id="r_id">
            <area shape="rect" coords="179,484,261,545" alt="Room7" href="~/Views/Room/Details/79" id="r_id">
            <area shape="rect" coords="178,549,261,611" alt="Room8" href="~/Views/Room/Details/80" id="r_id">
            <area shape="rect" coords="100,557,154,641" alt="Room9" href="~/Views/Room/Details/81" id="r_id">
            <area shape="rect" coords="178,615,262,671" alt="Room10" href="~/Views/Room/Details/82" id="r_id">
            <area shape="rect" coords="67,491,145,592" alt="Room11" href="~/Views/Room/Details/83" id="r_id">
            <area shape="rect" coords="65,427,148,486" alt="Room12" href="~/Views/Room/Details/84" id="r_id">
            <area shape="rect" coords="66,163,146,247" alt="Room13" href="~/Views/Room/Details/85" id="r_id">
            <area shape="rect" coords="67,75,146,157" alt="Room13" href="~/Views/Room/Details/86" id="r_id">
        </map>
    </div>

    @foreach (var item in Model)
    {
        <div class="col-8 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <form class="forms-sample">
                        <div class="form-group row">
                            <label for="exampleInputUsername2" class="col-sm-4 col-form-label">Building Name</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_bname)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputEmail2" class="col-sm-4 col-form-label">Floor Level</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_bid)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputMobile" class="col-sm-4 col-form-label">Address</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_address)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputPassword2" class="col-sm-4 col-form-label">Landlord Name</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_landlord)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">WiFi Password</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_wifipwd)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">Modem IP</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_modemip)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">CCTV QR Code</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_cctvqr)
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-auto">
                                @Html.ActionLink("Edit", "FloorList", null, new { @class = "btn btn-primary btn-rounded btn-fw" })
                            </div>
                            <div class="col-auto">
                                @Html.ActionLink("Back", "Index", null, new { @class = "btn btn-light btn-rounded btn-fw" })
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>

    }
</body>
</html>
