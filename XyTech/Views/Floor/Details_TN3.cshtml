
@model XyTech.Models.tb_floor

@{
    ViewBag.Title = "Details_TN3";
}

<!DOCTYPE html>
<html>
<body>
    <p class="card-title mb-0" style="font-size: x-large; font-weight:bold">Floor Information</p>
    <br />
    <div>
        <img class="floorlayout" src="~/Content/assets/images/floor_layout_trim/TN3-2.jpeg" alt="DL1" usemap="#DL1map">

        <map name="DL1map">
            <area shape="poly" coords="193,26,247,28,246,85,169,87,166,51,182,43" alt="Room1" href="@Url.Action("Details", "Room", new { id = 94 })" id="r_id">
            <area shape="rect" coords="168,91,244,154" alt="Room2" href="@Url.Action("Details", "Room", new { id = 95 })" id="r_id">
            <area shape="rect" coords="168,161,244,216" alt="Room3" href="@Url.Action("Details", "Room", new { id = 96 })" id="r_id">
            <area shape="rect" coords="166,220,245,278" alt="Room4" href="@Url.Action("Details", "Room", new { id = 97 })" id="r_id">
            <area shape="rect" coords="167,285,246,343" alt="Room5" href="@Url.Action("Details", "Room", new { id = 98 })" id="r_id">
            <area shape="rect" coords="167,352,245,407" alt="Room6" href="@Url.Action("Details", "Room", new { id = 99 })" id="r_id">
            <area shape="rect" coords="168,411,246,474" alt="Room7" href="@Url.Action("Details", "Room", new { id = 100 })" id="r_id">
            <area shape="rect" coords="168,481,245,576" alt="Room7" href="@Url.Action("Details", "Room", new { id = 101 })" id="r_id">
            <area shape="poly" coords="191,584,184,601,168,606,168,650,246,649,247,584" alt="Room7" href="@Url.Action("Details", "Room", new { id = 102 })" id="r_id">
            <area shape="poly" coords="58,476,131,475,135,541,110,565,56,564" alt="Room7" href="@Url.Action("Details", "Room", new { id = 103 })" id="r_id">
            <area shape="rect" coords="58,409,136,469" alt="Room7" href="@Url.Action("Details", "Room", new { id = 104 })" id="r_id">
            <area shape="poly" coords="58,172,111,174,134,198,134,232,59,232" alt="Room7" href="@Url.Action("Details", "Room", new { id = 105 })" id="r_id">
            <area shape="poly" coords="132,167,60,165,59,93,99,89,102,49,111,37,134,39" alt="Room7" href="@Url.Action("Details", "Room", new { id = 106 })" id="r_id">
        </map>
    </div>

    <div class="col-7 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <form class="forms-sample">
                    <div class="form-group row">
                        <label for="exampleInputUsername2" class="col-sm-4 col-form-label">Building Name</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.fl_bname)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputEmail2" class="col-sm-4 col-form-label">Floor Level</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.fl_bid)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputMobile" class="col-sm-4 col-form-label">Address</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.fl_address)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputPassword2" class="col-sm-4 col-form-label">Landlord Name</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.tb_landlord.l_name)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">WiFi Password</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.fl_wifipwd)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">Modem IP</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.fl_modemip)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">CCTV QR Code</label>
                        <div class="col-sm-7 col-form-label">
                            <img src="@Url.Content("~/Content/assets/images/cctvqr/" + Model.fl_cctvqr)" alt="Uploaded Image" style="width: 300px; height: auto;" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-auto">
                            @Html.ActionLink("Edit", "FloorList", null, new { @class = "btn btn-primary btn-rounded btn-fw" })
                        </div>
                        <div class="col-auto">
                            @Html.ActionLink("Back", "Index", null, new { @class = "btn btn-light btn-rounded btn-fw" })
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>


</body>
</html>
