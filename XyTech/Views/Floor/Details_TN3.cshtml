
@model IEnumerable<XyTech.Models.tb_floor>

@{
    ViewBag.Title = "Details_TN3";
}

<!DOCTYPE html>
<html>
<body>
    <p class="card-title mb-0" style="font-size: x-large; font-weight:bold">Floor Information</p>
    <br /><h4 class="card-title">Floor Information</h4>
    <div>
        <img class="floorlayout" src="~/Content/assets/images/floor_layout_trim/TN3-2.jpeg" alt="DL1" usemap="#DL1map" width="300" height="800">

        <map name="DL1map">
            <area shape="poly" coords="168,29,245,85" alt="Room1" href="~/Views/Room/Details/94" id="r_id">
            <area shape="rect" coords="168,91,244,154" alt="Room2" href="~/Views/Room/Details/95" id="r_id">
            <area shape="rect" coords="168,161,244,216" alt="Room3" href="~/Views/Room/Details/96" id="r_id">
            <area shape="rect" coords="166,220,245,278" alt="Room4" href="~/Views/Room/Details/97" id="r_id">
            <area shape="rect" coords="167,285,246,343" alt="Room5" href="~/Views/Room/Details/98" id="r_id">
            <area shape="rect" coords="167,352,245,407" alt="Room6" href="~/Views/Room/Details/99" id="r_id">
            <area shape="rect" coords="168,411,246,474" alt="Room7" href="~/Views/Room/Details/100" id="r_id">
            <area shape="rect" coords="168,481,245,576" alt="Room7" href="~/Views/Room/Details/101" id="r_id">
            <area shape="rect" coords="170,582,246,648" alt="Room7" href="~/Views/Room/Details/102" id="r_id">
            <area shape="rect" coords="58,473,134,563" alt="Room7" href="~/Views/Room/Details/103" id="r_id">
            <area shape="rect" coords="58,409,136,469" alt="Room7" href="~/Views/Room/Details/104" id="r_id">
            <area shape="rect" coords="58,175,135,232" " alt="Room7" href="~/Views/Room/Details/105" id="r_id">
            <area shape="poly" coords="132,167,60,165,59,93,99,89,102,49,111,37,134,39" alt="Room7" href="~/Views/Room/Details/106" id="r_id">
        </map>
    </div>

    @foreach (var item in Model)
    {
        <div class="col-8 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <form class="forms-sample">
                        <div class="form-group row">
                            <label for="exampleInputUsername2" class="col-sm-4 col-form-label">Building Name</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_bname)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputEmail2" class="col-sm-4 col-form-label">Floor Level</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_bid)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputMobile" class="col-sm-4 col-form-label">Address</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_address)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputPassword2" class="col-sm-4 col-form-label">Landlord Name</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_landlord)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">WiFi Password</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_wifipwd)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">Modem IP</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_modemip)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">CCTV QR Code</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_cctvqr)
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-auto">
                                @Html.ActionLink("Edit", "FloorList", null, new { @class = "btn btn-primary btn-rounded btn-fw" })
                            </div>
                            <div class="col-auto">
                                @Html.ActionLink("Back", "Index", null, new { @class = "btn btn-light btn-rounded btn-fw" })
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>

    }
</body>
</html>
