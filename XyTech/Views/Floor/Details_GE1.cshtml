@model XyTech.Models.tb_floor

@{
    ViewBag.Title = "Details_GE1";
}

<!DOCTYPE html>
<html>
<body>
    <p class="card-title mb-0" style="font-size: x-large; font-weight:bold">Floor Information</p>
    <br />
    <div>
        <img class="floorlayout" src="~/Content/assets/images/floor_layout_trim/GE1-1.jpeg" alt="DL1" usemap="#DL1map">

        <map name="DL1map">
            <area shape="rect" coords="52,48,121,129" alt="Room1" href="@Url.Action("Details", "Room", new { id = 30 })" id="r_id">
            <area shape="rect" coords="53,138,120,220" alt="Room2" href="@Url.Action("Details", "Room", new { id = 31 })" id="r_id">
            <area shape="rect" coords="53,231,150,286" alt="Room3" href="@Url.Action("Details", "Room", new { id = 32 })" id="r_id">
            <area shape="rect" coords="52,296,150,350" alt="Room4" href="@Url.Action("Details", "Room", new { id = 33 })" id="r_id">
            <area shape="rect" coords="55,361,137,417" alt="Room5" href="@Url.Action("Details", "Room", new { id = 34 })" id="r_id">
            <area shape="rect" coords="53,423,140,479" alt="Room6" href="@Url.Action("Details", "Room", new { id = 35 })" id="r_id">
            <area shape="rect" coords="55,487,139,539" alt="Room7" href="@Url.Action("Details", "Room", new { id = 36 })" id="r_id">
            <area shape="rect" coords="51,545,103,657" alt="Room8" href="@Url.Action("Details", "Room", new { id = 37 })" id="r_id">
            <area shape="poly" coords="107,572,135,571,161,597,160,654,106,656" alt="Room9" href="@Url.Action("Details", "Room", new { id = 38 })" id="r_id">
            <area shape="poly" coords="193,476,256,477,257,526,169,524,169,500,187,496" alt="Room10" href="@Url.Action("Details", "Room", new { id = 39 })" id="r_id">
            <area shape="rect" coords="170,425,257,471" alt="Room11" href="@Url.Action("Details", "Room", new { id = 40 })" id="r_id">
            <area shape="rect" coords="153,142,256,221" alt="Room12" href="@Url.Action("Details", "Room", new { id = 41 })" id="r_id">
            <area shape="rect" coords="155,45,216,140" alt="Room13" href="@Url.Action("Details", "Room", new { id = 42 })" id="r_id">
        </map>
    </div>

    <div class="col-7 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <form class="forms-sample">
                    <div class="form-group row">
                        <label for="exampleInputUsername2" class="col-sm-4 col-form-label">Building Name</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.fl_bname)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputEmail2" class="col-sm-4 col-form-label">Floor Level</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.fl_bid)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputMobile" class="col-sm-4 col-form-label">Address</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.fl_address)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputPassword2" class="col-sm-4 col-form-label">Landlord Name</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.tb_landlord.l_name)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">WiFi Password</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.fl_wifipwd)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">Modem IP</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.fl_modemip)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">CCTV QR Code</label>
                        <div class="col-sm-7 col-form-label">
                            <img src="@Url.Content("~/Content/assets/images/cctvqr/" + Model.fl_cctvqr)" alt="Uploaded Image" style="width: 300px; height: auto;" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-auto">
                            @Html.ActionLink("Edit", "FloorList", null, new { @class = "btn btn-primary btn-rounded btn-fw" })
                        </div>
                        <div class="col-auto">
                            @Html.ActionLink("Back", "Index", null, new { @class = "btn btn-light btn-rounded btn-fw" })
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</body>
</html>
