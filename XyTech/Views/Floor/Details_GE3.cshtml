
@model IEnumerable<XyTech.Models.tb_floor>

@{
    ViewBag.Title = "Details_GE3";
}

<!DOCTYPE html>
<html>
<body>
    <p class="card-title mb-0" style="font-size: x-large; font-weight:bold">Floor Information</p>
    <br />
    <div>
        <img class="floorlayout" src="~/Content/assets/images/floor_layout_trim/GE3-2.jpeg" alt="DL1" usemap="#DL1map" width="300" height="800">

        <map name="DL1map">
            <area shape="rect" coords="46,31,114,130" alt="Room1" href="~/Views/Room/Details/57" id="r_id">
            <area shape="rect" coords="44,135,114,207" alt="Room2" href="~/Views/Room/Details/58" id="r_id">
            <area shape="rect" coords="46,215,144,267" alt="Room3" href="~/Views/Room/Details/59" id="r_id">
            <area shape="rect" coords="44,277,142,329" alt="Room4" href="~/Views/Room/Details/60" id="r_id">
            <area shape="rect" coords="43,335,130,399" alt="Room5" href="~/Views/Room/Details/61" id="r_id">
            <area shape="rect" coords="44,403,130,463" alt="Room6" href="~/Views/Room/Details/62" id="r_id">
            <area shape="rect" coords="44,465,131,525" alt="Room7" href="~/Views/Room/Details/63" id="r_id">
            <area shape="rect" coords="43,529,96,641" alt="Room8" href="~/Views/Room/Details/64" id="r_id">
            <area shape="rect" coords="100,557,154,641" alt="Room9" href="~/Views/Room/Details/65" id="r_id">
            <area shape="rect" coords="160,463,249,509" alt="Room10" href="~/Views/Room/Details/66" id="r_id">
            <area shape="rect" coords="159,406,248,457" alt="Room11" href="~/Views/Room/Details/67" id="r_id">
            <area shape="rect" coords="148,127,248,208" alt="Room12" href="~/Views/Room/Details/68" id="r_id">
            <area shape="rect" coords="144,31,208,122" alt="Room13" href="~/Views/Room/Details/69" id="r_id">
        </map>
    </div>

    @foreach (var item in Model)
    {
        <div class="col-8 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <form class="forms-sample">
                        <div class="form-group row">
                            <label for="exampleInputUsername2" class="col-sm-4 col-form-label">Building Name</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_bname)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputEmail2" class="col-sm-4 col-form-label">Floor Level</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_bid)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputMobile" class="col-sm-4 col-form-label">Address</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_address)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputPassword2" class="col-sm-4 col-form-label">Landlord Name</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_landlord)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">WiFi Password</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_wifipwd)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">Modem IP</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_modemip)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">CCTV QR Code</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_cctvqr)
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-auto">
                                @Html.ActionLink("Edit", "FloorList", null, new { @class = "btn btn-primary btn-rounded btn-fw" })
                            </div>
                            <div class="col-auto">
                                @Html.ActionLink("Back", "Index", null, new { @class = "btn btn-light btn-rounded btn-fw" })
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>

    }
</body>
</html>
