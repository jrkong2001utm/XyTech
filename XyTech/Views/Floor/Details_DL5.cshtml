@model IEnumerable<XyTech.Models.tb_floor>

@{
    ViewBag.Title = "Details_DL5";
}

<!DOCTYPE html>
<html>
<body>
    <p class="card-title mb-0" style="font-size: x-large; font-weight:bold">Floor Information</p>
    <br />
    <div>
        <img class="floorlayout" src="~/Content/assets/images/floor_layout_trim/DL5-2.jpeg" alt="DL1" usemap="#DL1map" width="300" height="800">

            <map name="DL1map">
                <area shape="rect" coords="43,29,119,89" alt="Room1" href="~/Views/Room/Details/15" id="r_id">
                <area shape="rect" coords="46,94,121,148" alt="Room2" href="~/Views/Room/Details/16" id="r_id">
                <area shape="rect" coords="45,154,122,212" alt="Room3" href="~/Views/Room/Details/17" id="r_id">
                <area shape="rect" coords="44,218,120,270" alt="Room4" href="~/Views/Room/Details/18" id="r_id">
                <area shape="rect" coords="44,278,124,329" alt="Room5" href="~/Views/Room/Details/19" id="r_id">
                <area shape="rect" coords="46,336,120,389" alt="Room6" href="~/Views/Room/Details/20" id="r_id">
                <area shape="rect" coords="45,398,120,454" alt="Room7" href="~/Views/Room/Details/21" id="r_id">
                <area shape="rect" coords="43,458,119,514" alt="Room8" href="~/Views/Room/Details/22" id="r_id">
                <area shape="rect" coords="46,520,122,573" alt="Room9" href="~/Views/Room/Details/23" id="r_id">
                <area shape="rect" coords="46,582,120,635" alt="Room10" href="~/Views/Room/Details/24" id="r_id">
                <area shape="rect" coords="154,514,230,560" alt="Room11" href="~/Views/Room/Details/25" id="r_id">
                <area shape="rect" coords="157,457,230,506" alt="Room12" href="~/Views/Room/Details/26" id="r_id">
                <area shape="rect" coords="154,398,230,448" alt="Room13" href="~/Views/Room/Details/27" id="r_id">
                <area shape="rect" coords="154,140,228,208" alt="Room14" href="~/Views/Room/Details/28" id="r_id">
                <area shape="rect" coords="154,140,228,208" alt="Room15" href="~/Views/Room/Details/29" id="r_id">
            </map>
    </div>

    @foreach (var item in Model)
    {
        <div class="col-8 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <form class="forms-sample">
                        <div class="form-group row">
                            <label for="exampleInputUsername2" class="col-sm-4 col-form-label">Building Name</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_bname)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputEmail2" class="col-sm-4 col-form-label">Floor Level</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_bid)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputMobile" class="col-sm-4 col-form-label">Address</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_address)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputPassword2" class="col-sm-4 col-form-label">Landlord Name</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_landlord)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">WiFi Password</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_wifipwd)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">Modem IP</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_modemip)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">CCTV QR Code</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_cctvqr)
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-auto">
                                @Html.ActionLink("Edit", "FloorList", null, new { @class = "btn btn-primary btn-rounded btn-fw" })
                            </div>
                            <div class="col-auto">
                                @Html.ActionLink("Back", "Index", null, new { @class = "btn btn-light btn-rounded btn-fw" })
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>

    }
</body>
</html>
