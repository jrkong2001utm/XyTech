
@model IEnumerable<XyTech.Models.tb_floor>

@{
    ViewBag.Title = "Details_GE2";
}

<!DOCTYPE html>
<html>
<body>
    <h4 class="card-title">Floor Information</h4>
    <div>
        <img class="floorlayout" src="~/Content/assets/images/floor_layout_trim/GE2-2.jpeg" alt="DL1" usemap="#DL1map" width="300" height="800">

        <map name="DL1map">
            <area shape="rect" coords="60,36,128,95" alt="Room1" href="~/Views/Room/Details/43" id="r_id">
            <area shape="rect" coords="58,100,128,158" alt="Room2" href="~/Views/Room/Details/44" id="r_id">
            <area shape="rect" coords="58,165,130,220" alt="Room3" href="~/Views/Room/Details/45" id="r_id">
            <area shape="rect" coords="60,223,161,282" alt="Room4" href="~/Views/Room/Details/46" id="r_id">
            <area shape="rect" coords="60,288,160,344" alt="Room5" href="~/Views/Room/Details/47" id="r_id">
            <area shape="rect" coords="60,352,149,408" alt="Room6" href="~/Views/Room/Details/48" id="r_id">
            <area shape="rect" coords="61,411,146,469" alt="Room7" href="~/Views/Room/Details/49" id="r_id">
            <area shape="rect" coords="61,476,146,532" alt="Room8" href="~/Views/Room/Details/50" id="r_id">
            <area shape="rect" coords="60,536,110,650" alt="Room9" href="~/Views/Room/Details/51" id="r_id">
            <area shape="rect" coords="116,566,169,646" alt="Room10" href="~/Views/Room/Details/52" id="r_id">
            <area shape="rect" coords="176,470,266,516" alt="Room11" href="~/Views/Room/Details/53" id="r_id">
            <area shape="rect" coords="176,416,264,463" alt="Room12" href="~/Views/Room/Details/54" id="r_id">
            <area shape="rect" coords="163,136,262,214" alt="Room13" href="~/Views/Room/Details/55" id="r_id">
            <area shape="rect" coords="160,38,222,130" alt="Room13" href="~/Views/Room/Details/56" id="r_id">
        </map>
    </div>

    @foreach (var item in Model)
    {
        <div class="col-8 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <form class="forms-sample">
                        <div class="form-group row">
                            <label for="exampleInputUsername2" class="col-sm-4 col-form-label">Building Name</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_bname)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputEmail2" class="col-sm-4 col-form-label">Floor Level</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_bid)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputMobile" class="col-sm-4 col-form-label">Address</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_address)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputPassword2" class="col-sm-4 col-form-label">Landlord Name</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_landlord)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">WiFi Password</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_wifipwd)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">Modem IP</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_modemip)
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">CCTV QR Code</label>
                            <div class="col-sm-7 col-form-label">
                                @Html.DisplayFor(modelItem => item.fl_cctvqr)
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-auto">
                                @Html.ActionLink("Edit", "FloorList", null, new { @class = "btn btn-primary btn-rounded btn-fw" })
                            </div>
                            <div class="col-auto">
                                @Html.ActionLink("Back", "Index", null, new { @class = "btn btn-light btn-rounded btn-fw" })
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>

    }
</body>
</html>
