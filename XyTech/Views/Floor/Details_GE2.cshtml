
@model XyTech.Models.tb_floor

@{
    ViewBag.Title = "Details_GE2";
}

<!DOCTYPE html>
<html>
<body>
    <p class="card-title mb-0" style="font-size: x-large; font-weight:bold">Floor Information</p>
    <br />
    <div>
        <img class="floorlayout" src="~/Content/assets/images/floor_layout_trim/GE2-2.jpeg" alt="DL1" usemap="#DL1map">

        <map name="DL1map">
            <area shape="rect" coords="60,36,128,95" alt="Room1" href="@Url.Action("Details", "Room", new { id = 43 })" id="r_id">
            <area shape="rect" coords="58,100,128,158" alt="Room2" href="@Url.Action("Details", "Room", new { id = 44 })" id="r_id">
            <area shape="rect" coords="58,165,130,220" alt="Room3" href="@Url.Action("Details", "Room", new { id = 45 })" id="r_id">
            <area shape="rect" coords="60,223,161,282" alt="Room4" href="@Url.Action("Details", "Room", new { id = 46 })" id="r_id">
            <area shape="rect" coords="60,288,160,344" alt="Room5" href="@Url.Action("Details", "Room", new { id = 47 })" id="r_id">
            <area shape="rect" coords="60,352,149,408" alt="Room6" href="@Url.Action("Details", "Room", new { id = 48 })" id="r_id">
            <area shape="rect" coords="61,411,146,469" alt="Room7" href="@Url.Action("Details", "Room", new { id = 49 })" id="r_id">
            <area shape="rect" coords="61,476,146,532" alt="Room8" href="@Url.Action("Details", "Room", new { id = 50 })" id="r_id">
            <area shape="poly" coords="60,536,134,535,126,548,122,559,111,558,108,646,58,646" alt="Room9" href="@Url.Action("Details", "Room", new { id = 51 })" id="r_id">
            <area shape="poly" coords="116,566,144,564,153,580,167,589,166,646,114,645" alt="Room10" href="@Url.Action("Details", "Room", new { id = 52 })" id="r_id">
            <area shape="poly" coords="200,468,263,470,264,516,176,516,176,490,191,484" alt="Room11" href="@Url.Action("Details", "Room", new { id = 53 })" id="r_id">
            <area shape="rect" coords="176,416,264,463" alt="Room12" href="@Url.Action("Details", "Room", new { id = 54 })" id="r_id">
            <area shape="rect" coords="163,136,262,214" alt="Room13" href="@Url.Action("Details", "Room", new { id = 55 })" id="r_id">
            <area shape="rect" coords="160,38,222,130" alt="Room13" href="@Url.Action("Details", "Room", new { id = 56 })" id="r_id">
        </map>
    </div>

    <div class="col-7 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <form class="forms-sample">
                    <div class="form-group row">
                        <label for="exampleInputUsername2" class="col-sm-4 col-form-label">Building Name</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.fl_bname)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputEmail2" class="col-sm-4 col-form-label">Floor Level</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.fl_bid)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputMobile" class="col-sm-4 col-form-label">Address</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.fl_address)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputPassword2" class="col-sm-4 col-form-label">Landlord Name</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.tb_landlord.l_name)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">WiFi Password</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.fl_wifipwd)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">Modem IP</label>
                        <div class="col-sm-7 col-form-label">
                            @Html.DisplayFor(model => model.fl_modemip)
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">CCTV QR Code</label>
                        <div class="col-sm-7 col-form-label">
                            <img src="@Url.Content("~/Content/assets/images/cctvqr/" + Model.fl_cctvqr)" alt="Uploaded Image" style="width: 300px; height: auto;" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="exampleInputConfirmPassword2" class="col-sm-4 col-form-label">Preset Message</label>
                        <div class="col-sm-7 col-form-label">
                            [
                            <br />
                            (attached layout bilik GE2)
                            <br />
                            dalam gambar 👆🏼
                            <br />
                            --> harga sewa bulanan
                            <br />
                            termasuk api dan air
                            <br />
                            --> bilik masih kosong
                            <br />
                            yang tidak bertanda ❌
                            <br />
                            <br />
                            + deposit RM100
                            <br />
                            <br />
                            Tingkat 2
                            <br />
                            <br />
                            video bilik:<br />
                            https://youtube.com/playlist?list=PLPU6s1lrzEXJvtz7dMltEwORganT1OqZs
                            <br />
                            <br />
                            lokasi:<br />
                            https://maps.app.goo.gl/S585ppM3axYT3Xv16
                            <br />
                            <br />
                            di atas Klinik Dr. Hani
                            <br />
                            ]
                        </div>
                    </div>
                    <div style="margin-top: 25px;">
                        @Html.ActionLink("Edit", "Edit", new { id = Model.fl_id }, new { @class = "btn btn-primary btn-rounded btn-fw" })
                        @Html.ActionLink("See Rooms", "Index", "Room", new { id = Model.fl_id }, new { @class = "btn btn-dark btn-rounded btn-fw" })
                        @Html.ActionLink("Back to Floor List", "FloorList", null, new { @class = "btn btn-light btn-rounded btn-fw" })
                    </div>
                </form>
            </div>
        </div>
    </div>

</body>
</html>
